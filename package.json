{
    "name": "teneno-api",
    "version": "1.6.0",
    "description": "The official Teneno API application",
    "author": "Tran Nguyen Thuong Truong <thuongtruong1009@proton.me>",
    "homepage": "https://github.com/thuongtruong1009/teneno-api#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/thuongtruong1009/teneno-api"
    },
    "funding": [{
            "type": "github",
            "url": "https://github.com/sponsors/thuongtruong1009"
        },
        {
            "type": "paypal",
            "url": "https://paypal.me/thuongtruong1009"
        },
        {
            "type": "momo",
            "url": "https://nhantien.momo.vn/0917085937"
        },
        {
            "type": "opencollective",
            "url": "https://opencollective.com/thuongtruong1009"
        }
    ],
    "license": "MIT",
    "keywords": [
        "teneno",
        "nestjs",
        "prisma",
        "postgre",
        "API",
        "typescript",
        "express",
        "docker",
        "github",
        "chat",
        "socket"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "webpack": "nest build --webpack --webpackPath webpack-hmr.config.ts --watch",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test,client}/**/*.ts\" --fix",
        "tslint": "tslint --fix -c tslint.json 'lib/**/*{.ts,.tsx}'",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky install",
        "pre-commit": "lint-staged",
        "prisma:pull": "prisma db pull",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev --create-only",
        "prisma:push": "npx prisma db push",
        "prisma:deploy": "npx prisma migrate deploy",
        "prisma:studio": "npx prisma studio",
        "prisma:seed": "npx prisma db seed --preview-feature",
        "prisma:reset": "npx prisma migrate reset",
        "clear": "npm rm -rf node_modules && npm cache clean --force && npm install",
        "documentation": "npx @compodoc/compodoc -p tsconfig.json -s -n \"Teneno API documentation\" -r 5500 -w -o --hideGenerator --customFavicon \"/public/favicon.ico\" --theme \"postmark\""
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    },
    "dependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.2.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/platform-socket.io": "^9.0.9",
        "@nestjs/serve-static": "^3.0.0",
        "@nestjs/swagger": "^6.0.4",
        "@nestjs/websockets": "^9.0.9",
        "@prisma/client": "^4.2.1",
        "@types/passport-jwt": "^3.0.6",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "helmet": "^6.0.0",
        "husky": "^8.0.1",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.0",
        "prisma": "^4.2.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "socket.io": "^4.5.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.1.19",
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.4",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.0.0",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "28.1.2",
        "lint-staged": "^13.0.3",
        "prettier": "2.7.1",
        "run-script-webpack-plugin": "^0.1.1",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^4.3.5",
        "webpack": "^5.74.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}